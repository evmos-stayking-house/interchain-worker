{
  "executionRoleArn": "arn:aws:iam::898329626477:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bq-scheduled-worker-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "command": [
        "evmos:liquidation:tx",
        "--vault",
        "{테스트 환경 Vault Address}"
      ],
      "linuxParameters": {
        "initProcessEnabled": true
      },
      "environment": [
        {
          "name": "APP_ENV",
          "value": "development"
        }
      ],
      "secrets": [
        {
          "name": "BOT_PRIVATE_KEY",
          "valueFrom": "BOT_PRIVATE_KEY"
        },
        {
          "name": "STAYKING_CONTRACT_ADDRESS",
          "valueFrom": "STAYKING_CONTRACT_ADDRESS"
        }
      ],
      "cpu": 256,
      "memory": null,
      "memoryReservation": 512,
      "essential": true,
      "ulimits": [
        {
          "softLimit": 65535,
          "hardLimit": 65535,
          "name": "nofile"
        }
      ],
      "image": "898329626477.dkr.ecr.ap-northeast-2.amazonaws.com/sooho/bq/scheduled-worker",
      "name": "scheduled-worker"
    }
  ],
  "taskRoleArn": "arn:aws:iam::898329626477:role/ecsTaskExecutionRole",
  "family": "bq-scheduled-worker-task",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
